From 7491dbfdc246e3f809453699a85bb8ac47cccfcc Mon Sep 17 00:00:00 2001
From: kornstae <none@none>
Date: Mon, 11 Mar 2002 10:28:51 +0000
Subject: [PATCH] Use Queue::RemoveNthElement instead of Queue::Remove for
 efficiency.

---
 generic/RootSet.cc | 3 +--
 1 file changed, 1 insertion(+), 2 deletions(-)

diff --git a/generic/RootSet.cc b/generic/RootSet.cc
index f96fde3cd..dcb700ba6 100644
--- a/generic/RootSet.cc
+++ b/generic/RootSet.cc
@@ -69,8 +69,7 @@ class Set: private Queue {
     for (u_int i = GetNumberOfElements(); i--; ) {
       Element *element = Element::FromWordDirect(GetNthElement(i));
       if (element->GetPointer() == &root) {
-	//--** inefficient: use some method Queue::RemoveNthElement
-  	Queue::Remove(element->ToWord());
+	Queue::RemoveNthElement(i);
   	break;
       }
     }
