---
 gdal/ogr/ogrsf_frmts/s57/ogrs57datasource.cpp | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/gdal/ogr/ogrsf_frmts/s57/ogrs57datasource.cpp b/gdal/ogr/ogrsf_frmts/s57/ogrs57datasource.cpp
index 5b3694156b7b..197e68c831b4 100644
--- a/gdal/ogr/ogrsf_frmts/s57/ogrs57datasource.cpp
+++ b/gdal/ogr/ogrsf_frmts/s57/ogrs57datasource.cpp
@@ -202,7 +202,7 @@ int OGRS57DataSource::Open( const char * pszFilename )
                              GetOption(S57O_RECODE_BY_DSSI) );
 
     S57Reader *poModule = new S57Reader( pszFilename );
-    bool bRet = poModule->SetOptions( papszReaderOptions );
+    bool bRet = static_cast<bool>( poModule->SetOptions( papszReaderOptions ) );
     CSLDestroy( papszReaderOptions );
 
     if( !bRet )
