From b9f498d54ee713fd059b6f176455767399d1c9b5 Mon Sep 17 00:00:00 2001
From: Gleb Belov <gleb.belov@monash.edu>
Date: Mon, 3 Jun 2019 15:31:33 +1000
Subject: [PATCH] SCIP: wall time for solving

---
 solvers/MIP/MIP_scip_wrap.cpp | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/solvers/MIP/MIP_scip_wrap.cpp b/solvers/MIP/MIP_scip_wrap.cpp
index 857de51f4..352261c9c 100644
--- a/solvers/MIP/MIP_scip_wrap.cpp
+++ b/solvers/MIP/MIP_scip_wrap.cpp
@@ -581,12 +581,16 @@ SCIP_RETCODE MIP_scip_wrapper::solve_SCIP() {  // Move into ancestor?
      SCIP_CALL( SCIPwriteParams (scip, options->sReadParams.c_str(), TRUE, FALSE) );
     }
 
+    cbui.pOutput->dWallTime0 = output.dWallTime0 =
+      std::chrono::steady_clock::now();
    output.dCPUTime = clock();
 
    /* Optimize the problem and obtain solution. */
    SCIP_CALL( SCIPsolve (scip) );
 //    wrap_assert( !retcode,  "Failed to optimize MIP." );
 
+   output.dWallTime = std::chrono::duration<double>(
+     std::chrono::steady_clock::now() - output.dWallTime0).count();
    output.dCPUTime = (clock() - output.dCPUTime) / CLOCKS_PER_SEC;
    
    cbuiPtr = 0;                             /// cleanup
