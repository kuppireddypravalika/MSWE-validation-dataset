---
 .../src/extensions/hexplanetgeneration/utils/tools.cpp          | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/Extensions/src/extensions/hexplanetgeneration/utils/tools.cpp b/src/Extensions/src/extensions/hexplanetgeneration/utils/tools.cpp
index 4d877e4eb..f00cfaedc 100644
--- a/src/Extensions/src/extensions/hexplanetgeneration/utils/tools.cpp
+++ b/src/Extensions/src/extensions/hexplanetgeneration/utils/tools.cpp
@@ -144,7 +144,7 @@ Vector3 Tools::setLength(Vector3&& v, float len)
 {
   v.normalize();
   v *= len;
-  return v;
+  return std::move(v);
 }
 
 Color4 Tools::ocv(unsigned int val)
