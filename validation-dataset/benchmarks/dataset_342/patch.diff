From b595487d23ee2db75936806e199c59eaa63bda47 Mon Sep 17 00:00:00 2001
From: Kristian Bendiksen <kristian.bendiksen@gmail.com>
Date: Wed, 13 Mar 2024 10:56:58 +0100
Subject: [PATCH] #11285 GRPC: fix high CPU usage in console application

Increase polling interval to avoid using one core constantly.

Fixes #11285.
---
 ApplicationExeCode/RiaGrpcConsoleApplication.cpp | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/ApplicationExeCode/RiaGrpcConsoleApplication.cpp b/ApplicationExeCode/RiaGrpcConsoleApplication.cpp
index b0e17ee9abf..dae4ef68d27 100644
--- a/ApplicationExeCode/RiaGrpcConsoleApplication.cpp
+++ b/ApplicationExeCode/RiaGrpcConsoleApplication.cpp
@@ -46,7 +46,7 @@ RiaGrpcConsoleApplication::RiaGrpcConsoleApplication( int& argc, char** argv )
 {
     m_idleTimer = new QTimer( this );
     connect( m_idleTimer, SIGNAL( timeout() ), this, SLOT( doIdleProcessing() ) );
-    m_idleTimer->start( 0 );
+    m_idleTimer->start( 5 );
 }
 
