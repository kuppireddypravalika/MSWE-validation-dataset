From b5392fdb79d427b57d2b5645ab40f914ad530891 Mon Sep 17 00:00:00 2001
From: Karthik Krishnan <karthik.krishnan@kitware.com>
Date: Fri, 12 Aug 2005 12:16:03 -0400
Subject: [PATCH] BUG: Bug #2130 - RGBPixel should have an empty constructor
 for faster creation of rgb images

---
 Code/Common/itkRGBPixel.h | 5 +++--
 1 file changed, 3 insertions(+), 2 deletions(-)

diff --git a/Code/Common/itkRGBPixel.h b/Code/Common/itkRGBPixel.h
index b8084fb169b..9c1609dba65 100644
--- a/Code/Common/itkRGBPixel.h
+++ b/Code/Common/itkRGBPixel.h
@@ -66,8 +66,9 @@ class RGBPixel: public FixedArray<TComponent,3>
   typedef TComponent ComponentType;
   typedef typename Superclass::ValueType ValueType;
   
-  /** Default constructor has nothing to do. */
-  RGBPixel() {this->Fill(0);}
+  /** Default constructor has nothing to do */
+  RGBPixel() {};
+  /** Constructor to fill Red=Blug=Green= r. */
   RGBPixel (const ComponentType& r) { this->Fill(r); }
   
   /** Pass-through constructor for the Array base class. */
