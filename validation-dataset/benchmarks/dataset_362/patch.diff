From 8597a440dce374a99d05df6423d4d77f4a5065e1 Mon Sep 17 00:00:00 2001
From: Matthias Kuehlewein <github@kuehlewein.de>
Date: Sun, 16 Jan 2022 18:17:49 +0100
Subject: [PATCH] TextSymbol: Optimize comparision of custom tabs

For comparing 'custom_tabs' and 'text->custom_tabs' which are both
of type std::vector<int> it is more efficient to use == which does
exactly was implemented before manually:
check the sizes and if both vectors are of equal length compare the
content.
---
 src/core/symbols/text_symbol.cpp | 12 ++----------
 1 file changed, 2 insertions(+), 10 deletions(-)

diff --git a/src/core/symbols/text_symbol.cpp b/src/core/symbols/text_symbol.cpp
index 7c64d43ea3..6580c9a9bf 100644
--- a/src/core/symbols/text_symbol.cpp
+++ b/src/core/symbols/text_symbol.cpp
@@ -1,6 +1,6 @@
 /*
  *    Copyright 2012, 2013 Thomas SchÃ¶ps
- *    Copyright 2012-2019 Kai Pastor
+ *    Copyright 2012-2022 Kai Pastor
  *
  *    This file is part of OpenOrienteering.
  *
@@ -508,15 +508,7 @@ bool TextSymbol::equalsImpl(const Symbol* other, Qt::CaseSensitivity case_sensit
 			line_below_distance != text->line_below_distance)
 			return false;
 	}
-	if (custom_tabs.size() != text->custom_tabs.size())
-		return false;
-	for (size_t i = 0, end = custom_tabs.size(); i < end; ++i)
-	{
-		if (custom_tabs[i] != text->custom_tabs[i])
-			return false;
-	}
-	
-	return true;
+	return (custom_tabs == text->custom_tabs);
 }
 
 QString TextSymbol::getIconText() const
