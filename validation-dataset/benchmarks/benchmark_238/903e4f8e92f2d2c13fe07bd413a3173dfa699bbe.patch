Subject: [PATCH] =?UTF-8?q?=E2=9A=A1=EF=B8=8F=20Improved=20std::hash=20of?=
 =?UTF-8?q?=20BitSets=20to=20use=20murmur=5Fhash=20instead.?=
MIME-Version: 1.0
Content-Type: text/plain; charset=UTF-8
Content-Transfer-Encoding: 8bit

---
 source/tools/BitSet.h | 7 +------
 1 file changed, 1 insertion(+), 6 deletions(-)

diff --git a/source/tools/BitSet.h b/source/tools/BitSet.h
index 679134ff9d..af80657062 100644
--- a/source/tools/BitSet.h
+++ b/source/tools/BitSet.h
@@ -1390,12 +1390,7 @@ namespace std
     {
         size_t operator()( const emp::BitSet<N>& bs ) const
         {
-           static const uint32_t NUM_BYTES = 1 + ((bs.GetSize() - 1) >> 3);
-           size_t result = bs.GetByte(0);
-           for (unsigned int i = 1; i < NUM_BYTES; ++i){
-                result = emp::hash_combine(result, bs.GetByte(i));
-           }
-           return result;
+          return emp::murmur_hash(bs.GetBytes());
         }
     };
 }
