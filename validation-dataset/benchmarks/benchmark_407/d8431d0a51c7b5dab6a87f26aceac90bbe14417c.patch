From d8431d0a51c7b5dab6a87f26aceac90bbe14417c Mon Sep 17 00:00:00 2001
From: Tobias Doerffel <tobias.doerffel@gmail.com>
Date: Mon, 13 Feb 2017 16:54:41 +0100
Subject: [PATCH] Core: Ldap: LdapDirectory: use std::sort instead of
 deprecated qSort

---
 core/src/Ldap/LdapDirectory.cpp | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/core/src/Ldap/LdapDirectory.cpp b/core/src/Ldap/LdapDirectory.cpp
index 7dc479688..f8cd97139 100644
--- a/core/src/Ldap/LdapDirectory.cpp
+++ b/core/src/Ldap/LdapDirectory.cpp
@@ -374,7 +374,7 @@ QStringList LdapDirectory::computerLabs(const QString &filterValue)
 
 	computerLabs.removeDuplicates();
 
-	qSort( computerLabs );
+	std::sort( computerLabs.begin(), computerLabs.end() );
 
 	return computerLabs;
 }
