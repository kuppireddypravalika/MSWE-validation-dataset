From b8f6af0dbb5a135cd3e15151109e3f1f08e91660 Mon Sep 17 00:00:00 2001
From: Jesse Allen <the3dfxdude@gmail.com>
Date: Wed, 6 Jul 2011 19:56:11 -0600
Subject: [PATCH] VgaSDL: Decrease the minimum time between frames.

---
 src/video/sdl/vga_sdl.cpp | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/video/sdl/vga_sdl.cpp b/src/video/sdl/vga_sdl.cpp
index f7fb2e04a..734613cc6 100644
--- a/src/video/sdl/vga_sdl.cpp
+++ b/src/video/sdl/vga_sdl.cpp
@@ -402,7 +402,7 @@ void VgaSDL::flip()
 {
    static Uint32 ticks = 0;
    Uint32 cur_ticks = SDL_GetTicks();
-   if (cur_ticks > ticks + 34 || cur_ticks < ticks) {
+   if (cur_ticks > ticks + 5 || cur_ticks < ticks) {
       SurfaceSDL *tmp = vga_front.get_buf();
       SDL_Surface *src = tmp->get_surface();
       ticks = cur_ticks;
