From 7d16560c9604ccc753fd76ddf0277aabc40a0324 Mon Sep 17 00:00:00 2001
From: dnbh <dnb@cs.jhu.edu>
Date: Mon, 15 Oct 2018 17:36:22 -0400
Subject: [PATCH] Use std::swap instead of new variables.

---
 bonsai/include/setcmp.h | 10 ++++------
 1 file changed, 4 insertions(+), 6 deletions(-)

diff --git a/bonsai/include/setcmp.h b/bonsai/include/setcmp.h
index a30e8010..52fa6368 100644
--- a/bonsai/include/setcmp.h
+++ b/bonsai/include/setcmp.h
@@ -11,12 +11,10 @@ template<typename KhashType>
 size_t intersection_size(const KhashType *a, const KhashType *b) {
     size_t ret(0);
     const KhashType *lhs, *rhs;
-    if(a->n_buckets <= b->n_buckets) {
-        lhs = a, rhs = b;
-    } else lhs = b, rhs = a;
-    for(khiter_t ki(0); ki != kh_end(lhs); ++ki)
-        if(kh_exist(lhs, ki))
-            ret += (khash_get(rhs, kh_key(lhs, ki)) != kh_end(rhs));
+    if(b->n_buckets <= a->n_buckets) std::swap(a, b);
+    for(khiter_t ki(0); ki != kh_end(a); ++ki)
+        if(kh_exist(a, ki))
+            ret += (khash_get(b, kh_key(a, ki)) != kh_end(b));
     return ret;
 }
 
