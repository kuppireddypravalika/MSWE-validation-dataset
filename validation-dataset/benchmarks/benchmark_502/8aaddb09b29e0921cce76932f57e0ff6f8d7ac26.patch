From: =?UTF-8?q?Beno=C3=AEt=20Bleuz=C3=A9?= <benoit.bleuze@inria.fr>
Date: Thu, 22 Mar 2012 15:27:33 +0100
Subject: [PATCH] Only use QList::reserve() when QT > 4.6.x

---
 src/medCore/medAbstractData.cpp | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/src/medCore/medAbstractData.cpp b/src/medCore/medAbstractData.cpp
index 45f892b84d..a7b603afd8 100644
--- a/src/medCore/medAbstractData.cpp
+++ b/src/medCore/medAbstractData.cpp
@@ -57,7 +57,9 @@ medDataIndex medAbstractData::dataIndex() const
 QList< medAttachedData * > medAbstractData::attachedData() const
 {
     QList< medAttachedData * > ret;
+#if QT_VERSION > 0x0406FF
     ret.reserve(d->attachedData.size());
+#endif
     foreach( medAttachedData * data, d->attachedData ) {
         ret.push_back(data);
     }
