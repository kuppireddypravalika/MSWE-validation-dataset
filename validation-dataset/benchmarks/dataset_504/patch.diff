From 28a4aa42ac1ece82fba9c4e1f23249096beca1eb Mon Sep 17 00:00:00 2001
From: Dmitry Tsarevich <dimhotepus@gmail.com>
Date: Sun, 27 Sep 2020 22:51:52 +0300
Subject: [PATCH] [perf] Use range for loop instead of iterators

---
 src/Layers/xrRender/DetailManager.cpp | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/src/Layers/xrRender/DetailManager.cpp b/src/Layers/xrRender/DetailManager.cpp
index df8cfd7f547..77cf0c85457 100644
--- a/src/Layers/xrRender/DetailManager.cpp
+++ b/src/Layers/xrRender/DetailManager.cpp
@@ -213,10 +213,10 @@ void CDetailManager::Unload()
 {
     hw_Unload();
 
-    for (DetailIt it = objects.begin(); it != objects.end(); it++)
+    for (CDetail* detailObject : objects)
     {
-        (*it)->Unload();
-        xr_delete(*it);
+        detailObject->Unload();
+        xr_delete(detailObject);
     }
 
     objects.clear();
