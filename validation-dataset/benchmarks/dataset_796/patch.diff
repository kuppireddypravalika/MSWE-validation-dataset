From 74c59d7d96c994d0cd554519c75af9b812c88453 Mon Sep 17 00:00:00 2001
From: Robert Osfield <robert@openscenegraph.com>
Date: Fri, 29 May 2020 11:04:37 +0100
Subject: [PATCH] Relaced push_back usage with emplace_back

---
 src/vsg/viewer/Viewer.cpp | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/src/vsg/viewer/Viewer.cpp b/src/vsg/viewer/Viewer.cpp
index e76c9cfd81..02097ba351 100644
--- a/src/vsg/viewer/Viewer.cpp
+++ b/src/vsg/viewer/Viewer.cpp
@@ -395,7 +395,7 @@ void Viewer::setupThreading()
                 }
             };
 
-            threads.push_back(std::thread(run, task, _frameBlock, _submissionCompleted));
+            threads.emplace_back(std::thread(run, task, _frameBlock, _submissionCompleted));
         }
         else if (task->commandGraphs.size() >= 1)
         {
@@ -478,9 +478,9 @@ void Viewer::setupThreading()
             for (uint32_t i = 0; i < task->commandGraphs.size(); ++i)
             {
                 if (i == 0)
-                    threads.push_back(std::thread(run_primary, sharedData, task->commandGraphs[i]));
+                    threads.emplace_back(std::thread(run_primary, sharedData, task->commandGraphs[i]));
                 else
-                    threads.push_back(std::thread(run_secondary, sharedData, task->commandGraphs[i]));
+                    threads.emplace_back(std::thread(run_secondary, sharedData, task->commandGraphs[i]));
             }
         }
     }
