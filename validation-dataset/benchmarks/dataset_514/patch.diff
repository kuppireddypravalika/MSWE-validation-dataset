From: Xottab-DUTY <xottab-duty@yandex.ru>
Date: Sat, 22 Apr 2023 15:59:42 +0300
Subject: [PATCH] utils/xrCompress/xrCompress.cpp: use emplace instead of
 insert and std::make_pair

---
 src/utils/xrCompress/xrCompress.cpp | 16 +++++++++-------
 1 file changed, 9 insertions(+), 7 deletions(-)

diff --git a/src/utils/xrCompress/xrCompress.cpp b/src/utils/xrCompress/xrCompress.cpp
index 206ef0c448f..84a46fe9053 100644
--- a/src/utils/xrCompress/xrCompress.cpp
+++ b/src/utils/xrCompress/xrCompress.cpp
@@ -268,13 +268,15 @@ void xrCompressor::CompressOne(LPCSTR path)
     if (!A)
     {
         // Register for future aliasing
-        ALIAS R;
-        R.path = xr_strdup(fn);
-        R.crc = c_crc32;
-        R.c_ptr = c_ptr;
-        R.c_size_real = c_size_real;
-        R.c_size_compressed = c_size_compressed;
-        aliases.insert(std::make_pair(R.c_size_real, R));
+        ALIAS R
+        {
+            /*.path =*/ xr_strdup(fn),
+            /*.crc =*/ c_crc32,
+            /*.c_ptr =*/ c_ptr,
+            /*.c_size_real =*/ c_size_real,
+            /*.c_size_compressed =*/ c_size_compressed
+        };
+        aliases.emplace(c_size_real, R);
     }
 
     FS.r_close(src);
