 src/stan/prob/distributions/normal.hpp | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/stan/prob/distributions/normal.hpp b/src/stan/prob/distributions/normal.hpp
index 75e907054a4..e8740a92e39 100644
--- a/src/stan/prob/distributions/normal.hpp
+++ b/src/stan/prob/distributions/normal.hpp
@@ -162,7 +162,7 @@ namespace stan {
       }
       
       if (include_summand<propto,T_scale>::value) 
-	lp -= y.size() * log(sigma);
+	lp -= multiply_log(y.size(),sigma);
       
       if (include_summand<propto>::value) 
 	lp += y.size() * NEG_LOG_SQRT_TWO_PI;
